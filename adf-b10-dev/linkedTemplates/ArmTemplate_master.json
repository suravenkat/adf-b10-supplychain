					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"TumblingWindowTrigger_properties_pipeline_parameters_StartDate": {
						"value": "[parameters('TumblingWindowTrigger_properties_pipeline_parameters_StartDate')]"
					},
					"TumblingWindowTrigger_properties_pipeline_parameters_EndDate": {
						"value": "[parameters('TumblingWindowTrigger_properties_pipeline_parameters_EndDate')]"
					}
