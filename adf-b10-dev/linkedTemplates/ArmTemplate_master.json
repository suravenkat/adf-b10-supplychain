{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-b10-dev"
		},
		"ls_AzureDataLakeStorageGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_AzureDataLakeStorageGen2'"
		},
		"ls_Target_database_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_Target_database'"
		},
		"ls_source_sql_server_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_source_sql_server'"
		},
		"ls_AzureDataLakeStorageGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsgen2b10dev.dfs.core.windows.net"
		},
		"ls_AzureKeyVault_for_SQL_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://adf-b10-keyvault.vault.azure.net/"
		},
		"ls_Rest_API_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://reqres.in/api/users"
		},
		"ls_keyVault_sql_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "sqlb10secret"
		},
		"ls_sql_systemAssignedIdentity_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserverb10dev.database.windows.net"
		},
		"ls_sql_systemAssignedIdentity_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldbdev"
		},
		"TumblingWindowTrigger_properties_pipeline_parameters_StartDate": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"TumblingWindowTrigger_properties_pipeline_parameters_EndDate": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-b10-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_AzureDataLakeStorageGen2_accountKey": {
						"value": "[parameters('ls_AzureDataLakeStorageGen2_accountKey')]"
					},
					"ls_Target_database_connectionString": {
						"value": "[parameters('ls_Target_database_connectionString')]"
					},
					"ls_source_sql_server_connectionString": {
						"value": "[parameters('ls_source_sql_server_connectionString')]"
					},
					"ls_AzureDataLakeStorageGen2_properties_typeProperties_url": {
						"value": "[parameters('ls_AzureDataLakeStorageGen2_properties_typeProperties_url')]"
					},
					"ls_AzureKeyVault_for_SQL_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_AzureKeyVault_for_SQL_properties_typeProperties_baseUrl')]"
					},
					"ls_Rest_API_properties_typeProperties_url": {
						"value": "[parameters('ls_Rest_API_properties_typeProperties_url')]"
					},
					"ls_sql_systemAssignedIdentity_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_systemAssignedIdentity_properties_typeProperties_server')]"
					},
					"ls_sql_systemAssignedIdentity_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_systemAssignedIdentity_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-b10-dev_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
